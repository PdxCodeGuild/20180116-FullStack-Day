x = int(input('Please enter your test score.: '))
letter_grade = ''
if x >= 90:
    letter_grade = 'A'
    print('Your letter grade is: ' + letter_grade)
elif x >= 80:
    letter_grade = 'B'
    print('Your letter grade is: ' + letter_grade)
elif x >= 70:
    letter_grade = 'C'
    print('Your letter grade is: ' + letter_grade)
elif x >= 60:
    letter_grade = ''
    print('Your letter grade is: ' + letter_grade)
elif x >= 50:
    letter_grade = 'A'
    print('Your letter grade is: ' + letter_grade)
else:
    letter_grade = 'A'
    print('Your letter grade is: ' + letter_grade)




